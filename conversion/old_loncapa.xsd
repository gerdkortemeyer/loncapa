            <xs:attribute name="clipheight" type="xs:int"/>
            <xs:attribute name="scaledwidth" type="xs:int"/>
            <xs:attribute name="scaledheight" type="xs:int"/>
            <xs:attribute name="transparent" type="xs:string"/>
