      <xs:attribute default="0" name="derivativeorder">
        <xs:annotation>
          <xs:documentation>
            Function
          </xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:int">
            <xs:enumeration value="0"/>
            <xs:enumeration value="1"/>
            <xs:enumeration value="2"/>
            <xs:enumeration value="-1"/>
          </xs:restriction>
        </xs:simpleType>
