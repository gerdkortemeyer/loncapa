        <xs:group ref="text"/>
      </xs:choice>
      <xs:attribute name="colspan" type="xs:int"/>
      <xs:attribute name="rowspan" type="xs:int"/>
      <xs:attribute name="align" type="html-align"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="th">
